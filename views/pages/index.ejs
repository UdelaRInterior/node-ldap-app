        <thead>
            <tr>
                <th>Sede</th>             
                <th>Secci&oacuten</th>                
                <th>Edificio</th>
                <th>Interno</th>
            </tr>
        </thead>
        <tbody>
            <% phoneNumbers.forEach(function(element) {%>
            <tr>
                <td><%=element.sede%></td>
                <td><%=element.seccion%></td>
                <td><%=element.edificio%></td>
                <td><%=element.interno%></td>
            </tr>
            <%})%>
        </tbody>
    </table>
</div>