<div class="container">
    <table class="table">
        <thead>
            <tr>
                <th>Sede</th>
                <th>Sitio web</th>  
                <th>Caracter&iacutestica</th>
                <th>Tel&eacutefono</th>
            </tr>
        </thead>
        <tbody>
            <% sedes.forEach(function(element) {%>
            <tr>
                <td><%=element.nombre%></td>
                <td><a href="<%=element.webpage%>"><%=element.webpage%></a></td>
                <td><%=element.caracteristica%></td>
                <td><%=element.telefono%></td>
            </tr>
            <%})%>
        </tbody>
    </table> 
</div>

<div class="container">
